import random
caracteres =  "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

longitud = int(input("cuantos caracteres quieres en la contrase√±a"))
                   
password = ""

for i in range(longitud):
    x = random.choice(caracteres)
    password += x      

print (password)
